{
  "name": "lambda_node",
  "version": "1.0.0",
  "description": "node api with lambda and dinamo db",
  "keywords": [
    "node",
    "aws",
    "lambda",
    "typescript"
  ],
  "homepage": "https://github.com/celcito/lambda_node#readme",
  "bugs": {
    "url": "https://github.com/celcito/lambda_node/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celcito/lambda_node.git"
  },
  "license": "ISC",
  "author": "Celso Ricardo Alves",
  "type": "commonjs",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.ts",
    "start:dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "husky:prepare": "husky install",
    "test": "jest --passWithNoTests",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test:watch": "npm test --watch",
    "test:staged": "npm test --findRelatedTests",
    "test:push": "npm test --coverage"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
